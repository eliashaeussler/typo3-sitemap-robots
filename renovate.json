{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"local>eliashaeussler/renovate-config",
		"local>eliashaeussler/renovate-config:typo3-extension"
	],
	"packageRules": [
		{
			"matchFileNames": [
				"composer.json"
			],
			"constraints": {
				"php": "8.1.*"
			}
		},
		{
			"matchFileNames": [
				"Tests/CGL/composer.json"
			],
			"constraints": {
				"php": "8.3.*"
			}
		}
	]
}
